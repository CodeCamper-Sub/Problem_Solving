  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.clang_format_fallbackStyle": "LLVM",
  "editor.formatOnSave": false,
  "C_Cpp.clang_format_style": "file",
  "cph.language.cpp.Args": "-std=c++17",
  "cph.general.defaultLanguage": "cpp",
  "cph.general.firstTime": false,
  "cph.general.useShortCodeForcesName": true,
  "cph.general.saveLocation": "tmp",
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && clang++ -std=c++17 $fileName -o $workspaceRoot/tmp/$fileNameWithoutExt && $workspaceRoot/tmp/$fileNameWithoutExt",
    "python": "python3 -u"
  },
